package com.techouts.pojo;

import org.springframework.data.mongodb.core.mapping.Document;
import org.springframework.data.mongodb.core.mapping.Field;

@Document(collection = "UploadStatus")
public class ReportSummary {
	@Field("Collection Name")
	private String collectionName;
	@Field("Latest File Date")
	private String lastUploadedFileDate;

	public String getCollectionName() {
		return collectionName;
	}

	public void setCollectionName(String collectionName) {
		this.collectionName = collectionName;
	}

	public String getLastUploadedFileDate() {
		return lastUploadedFileDate;
	}

	public void setLastUploadedFileDate(String lastUploadedFileDate) {
		this.lastUploadedFileDate = lastUploadedFileDate;
	}

	@Override
	public String toString() {
		return "ReportSummary [collectionName=" + collectionName
				+ ", lastUploadedFileDate=" + lastUploadedFileDate + "]";
	}

}
