package com.techouts.hp.dto;

import org.springframework.data.mongodb.core.mapping.Document;
import org.springframework.data.mongodb.core.mapping.Field;

@Document(collection = "NEW_OPEN_ORDERS")
public class PcOpenorders {
	@Field("Date")
	private String date;
	@Field("CIA")
	private String CIA;
	@Field("Buyer")
	private String Buyer;
	@Field("item")
	private String item;
	@Field("Description")
	private String Description;
	@Field("PODate")
	private String PODate;
	@Field("PO #")
	private String PO;
	@Field("Line")
	private int Line;
	@Field("PO/Line")
	private String PO_Line;
	@Field("PO STATUS")
	private String PO_STATUS;
	@Field("Request Ship Date")
	private String Request_Ship_Date;
	@Field("Confirmed_delivery")
	private String Confirmed_delivery;
	@Field("ETD")
	private String ETD;
	@Field("Qty")
	private int Qty;
	@Field("Price")
	private double Price;
	@Field("Supplier")
	private String Supplier;
	@Field("Curr")
	private String Curr;

	@Field("Invoice# BaaN")
	private String Invoice_BaaN;
	@Field("Where used")
	private String where_used;
	@Field("Total")
	private double total;

	public String getCIA() {
		return CIA;
	}

	public void setCIA(String cIA) {
		CIA = cIA;
	}

	public String getBuyer() {
		return Buyer;
	}

	public void setBuyer(String buyer) {
		Buyer = buyer;
	}

	public String getItem() {
		return item;
	}

	public void setItem(String item) {
		this.item = item;
	}

	public String getDescription() {
		return Description;
	}

	public void setDescription(String description) {
		Description = description;
	}

	public String getPODate() {
		return PODate;
	}

	public void setPODate(String pODate) {
		PODate = pODate;
	}

	public String getPO() {
		return PO;
	}

	public void setPO(String pO) {
		PO = pO;
	}

	public int getLine() {
		return Line;
	}

	public String getDate() {
		return date;
	}

	public void setDate(String date) {
		this.date = date;
	}

	public void setLine(int line) {
		Line = line;
	}

	public String getPO_Line() {
		return PO_Line;
	}

	public void setPO_Line(String pO_Line) {
		PO_Line = pO_Line;
	}

	public String getPO_STATUS() {
		return PO_STATUS;
	}

	public void setPO_STATUS(String pO_STATUS) {
		PO_STATUS = pO_STATUS;
	}

	public String getRequest_Ship_Date() {
		return Request_Ship_Date;
	}

	public void setRequest_Ship_Date(String request_Ship_Date) {
		Request_Ship_Date = request_Ship_Date;
	}

	public String getConfirmed_delivery() {
		return Confirmed_delivery;
	}

	public void setConfirmed_delivery(String confirmed_delivery) {
		Confirmed_delivery = confirmed_delivery;
	}

	public String getETD() {
		return ETD;
	}

	public void setETD(String eTD) {
		ETD = eTD;
	}

	public int getQty() {
		return Qty;
	}

	public void setQty(int qty) {
		Qty = qty;
	}

	public double getPrice() {
		return Price;
	}



	public void setPrice(double price) {
		Price = price;
	}

	public String getSupplier() {
		return Supplier;
	}

	public void setSupplier(String supplier) {
		Supplier = supplier;
	}

	public String getCurr() {
		return Curr;
	}

	public void setCurr(String curr) {
		Curr = curr;
	}

	public String getInvoice_BaaN() {
		return Invoice_BaaN;
	}

	public void setInvoice_BaaN(String invoice_BaaN) {
		Invoice_BaaN = invoice_BaaN;
	}

	public String getWhere_used() {
		return where_used;
	}

	public void setWhere_used(String where_used) {
		this.where_used = where_used;
	}

	public double getTotal() {
		return total;
	}

	public void setTotal(double total) {
		this.total = total;
	}

	@Override
	public String toString() {
		return "Openorders [date=" + date + ", CIA=" + CIA + ", Buyer=" + Buyer
				+ ", item=" + item + ", Description=" + Description
				+ ", PODate=" + PODate + ", PO=" + PO + ", Line=" + Line
				+ ", PO_Line=" + PO_Line + ", PO_STATUS=" + PO_STATUS
				+ ", Request_Ship_Date=" + Request_Ship_Date
				+ ", Confirmed_delivery=" + Confirmed_delivery + ", ETD=" + ETD
				+ ", Qty=" + Qty + ", Price=" + Price + ", Supplier="
				+ Supplier + ", Curr=" + Curr + "]";
	}

}
