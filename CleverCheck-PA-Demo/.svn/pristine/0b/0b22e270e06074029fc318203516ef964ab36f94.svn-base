</h:head>

<style>
	.row {
    margin-right: 0px;
    margin-left: 0px; 
}
.nop{
	padding-left: 0px;
	padding-right: 0px;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
				function reduceCharsForID() {
					$(".prodID").each(function(){
						var char = $(this).text();
						//console.log(char);
						var charLength = char.length;
						//console.log(charLength);
						if(charLength > 27) {
							var newChar = char.substring(0,26);
							//console.log(newChar);
							$(this).html('<span class="livText">'+newChar+'..</span>');
							$(this).append('<span class="delText" style="display: none;">'+char+'</span>');
							//$(this).addClass("hasDelText");
							$(this).addClass("hasDelText");
						}
					});
				}

				reduceCharsForID();
});
</script>
<h:body style="background-color: #fff; overflow-x: hidden;">
<!-- <ui:insert>
<ui:include src="/WEB-INF/include/mobileHeaderTemplate.xhtml"></ui:include>
</ui:insert> -->
<h:form styleClass="form-vertical">


	<div style=" background-color: #006DB4;border-bottom: 1px solid #004F8A;height: 60px;padding: 10px 0px 55px;position: fixed;top: 0;z-index: 9999;width: 100%;">
		<div class="container-fluid nop">
	    	<div class="row">
	    		<div class="col-xs-1" style="margin-top: 12px;padding-left: 2px;"><h:commandLink value="" action="search.xhtml?faces-redirect=true" style="width:0px;"><h:graphicImage value="resources/app/images/back.svg"></h:graphicImage></h:commandLink></div>
	    		<!-- <div class="col-xs-7"><h:graphicImage value="resources/app/images/logo-hp-white.png" style="width:150px;"/></div> -->
	    		<div class="col-xs-7" style="padding: 0px;padding-left: 0px;margin: 5px 0 0 0px;"><h:commandLink value="" action="#{homeBean.mobileHome}" style="width:0px;"><span style="color: #fff;font-size: 20px;font-weight: 600;margin-left: 0px;">CLEVERCHECK</span><span style="color: #fff;font-size: 17px;font-weight: 600;margin-left: -2px;"></span></h:commandLink></div>
	    		<div class="col-xs-1" style="padding: 0px;    margin: 27px 0 0 -250px;"><h:commandLink value="" immediate="true" action="#{homeBean.mobileHome}" style="width:0px;"><h:graphicImage value="Resources/app/images/Allgebra_Logo.png" style="width:75px;height:25px;margin: 2px 0 0 40px;"/></h:commandLink></div>
	    		<div class="col-xs-1" style="margin-top: 9px;padding-left: 0px;margin-left: 3px;"><a href="#{request.contextPath}/search.xhtml"><h:graphicImage value="resources/app/images/search.svg"></h:graphicImage></a></div>
	    		
