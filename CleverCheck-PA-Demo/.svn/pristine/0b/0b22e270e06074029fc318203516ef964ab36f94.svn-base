<h:form style="width:100%;margin-top:68px;">
	<p:outputLabel value="NO Records Found"
								rendered="#{homeBean.norecords == 'true'}" />
<ui:repeat var="s" value="#{homeBean.skusDetailsInfo}"  varStatus="status">
		<div  style="margin-bottom: 15px;">
			<h:commandLink actionListener= "#{homeBean.attrListener}" action="viewDetails.xhtml?faces-redirect=true"  style="color: #000;text-decoration: no">
				<f:attribute name="skuId" value="#{s.skuId}"/>
					<f:attribute name="count" value="#{s.count}"/>
				<h5 style="margin-left: 15px;color: #000;font-weight: 700;margin-bottom: 2px;margin-top:79px;">#{s.skuId}</h5>
				<div style="border: 2px solid #037CD5;border-radius: 10px;margin-right: 5px;margin-left: 5px;width:97%">
					<div class="row" style="border-bottom: 2px solid #ccc;margin-right: 0px;margin-left: 0px;">
						<div class="col-xs-3" style="margin-bottom: 10px;">
							<p:graphicImage value="Resources/app/images/laptop.png"
								rendered="#{s.imageType == 'NB' }" height="60" style=" margin-top: 15px;margin-left: -21px;" />

							<p:graphicImage value="Resources/app/images/sys.png"
								rendered="#{s.imageType == 'PC' }" height="60" style=" margin-top: 10px;" />

							<p:graphicImage value="Resources/app/images/lcd.png"
								rendered="#{s.imageType == 'WS' }" height="60" />

						</div>
						<div class="col-xs-5 nop" style="margin-bottom: 5px;" align="right">
							<h2 align="left" style="margin-top: 16px;margin-bottom: 5px;text-align: right;margin-left: 0em;font-weight: 700;">#{s.skuAvailability}</h2>
							<div style="font-weight: 700;font-size: 11px;    margin-left: 0em;">Clear to Build</div>
						</div>
						<div class="col-xs-4 nop" style="margin-bottom: 5px;" align="right">
							<h2 align="center" style="margin-top: 16px;margin-bottom: 5px;margin-left: -2em;font-weight: 700;text-align: right;padding-right: 11px;">#{s.pipeLineQuantity}</h2>
							<div class="text-center" style="font-weight: 700;font-size: 11px;text-align: right;padding-right: 9px;">#{s.pipeLineWeeks} W Pipeline</div>
						</div>
					</div>
					<div class="row">
						<div class="col-xs-7" style="padding-top: 20px;padding-left: 0px;">
							<ul style="margin-left: -3em;font-size: 10px;font-weight: normal;color: #737373">
								<ui:repeat var="conf" value="#{s.configuration}">
											<li style=" height: 14px;overflow: hidden;display: list-item;list-style-position: inside;">#{conf}</li>
										</ui:repeat>	
							</ul>
						</div>
						<div class="col-xs-5" style="padding-left: 0px; padding-top: 3px;">
						<small><p:outputLabel value="Shortage" rendered="#{s.skuAvailability == '0'}" style="margin-left: -1.2em;"/></small>
							<ul style="margin-left: -5em;margin-top: -1.5em;font-size: 82%;color: #737373;padding-top:10px;">
										<ui:repeat var="conf" value="#{s.shortage}">
											<li class="prodID" style="    height: 14px;overflow: hidden;display: list-item;list-style-position: inside;">#{conf}</li>
										</ui:repeat>
							</ul>
						</div>
					</div>
				</div>
			</h:commandLink>
		</div>
	</ui:repeat>
	</h:form>
