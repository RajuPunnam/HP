package com.techouts.pojo;

import java.util.Date;

public class CartItems {

	private SkuDetailsInfo skuDetailsInfo;
	private int quantity;
	private Date addTime;
	private String username;
	private String brazilTime;
	

	public String getBrazilTime() {
		return brazilTime;
	}

	public void setBrazilTime(String brazilTime) {
		this.brazilTime = brazilTime;
	}

	public SkuDetailsInfo getSkuDetailsInfo() {
		return skuDetailsInfo;
	}

	public void setSkuDetailsInfo(SkuDetailsInfo skuDetailsInfo) {
		this.skuDetailsInfo = skuDetailsInfo;
	}

	public int getQuantity() {
		return quantity;
	}

	public void setQuantity(int quantity) {
		this.quantity = quantity;
	}

	public Date getAddTime() {
		return addTime;
	}

	public void setAddTime(Date addTime) {
		this.addTime = addTime;
	}

	public String getUsername() {
		String[] usernames =username.split("@");
		return usernames[0];
	}

	public void setUsername(String username) {
		this.username = username;
	}

	@Override
	public String toString() {
		return "CartItems [skuDetailsInfo=" + skuDetailsInfo + ", quantity=" + quantity + ", addTime=" + addTime
				+ ", username=" + username + ", brazilTime=" + brazilTime + "]";
	}

}
