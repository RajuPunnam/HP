<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:p="http://primefaces.org/ui">
<h:head>
	<meta charset="utf-8" />
	<meta name="description" content="" />
	<meta name="author" content="" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<meta name="viewport"
		content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimal-ui" />
	<meta name="apple-mobile-web-app-status-bar-style" content="yes" />

	<title>hp CLEVER CHECK</title>
	<!-- theme fonts -->
	<link href="resources/assets/bootstrap/css/bootstrap.min.css"
		rel="stylesheet" />
	<link href="resources/assets/font-awesome/css/font-awesome.min.css"
		rel="stylesheet" />
	<link href="resources/css/app.css" rel="stylesheet" />
	<script src="resources/js/checkout.js"></script>
</h:head>

<style>
.ui-inputfield {
	margin: -9px 0px 1px 0px;
	height: 42px;
	border-color: none !important;
	border: 1px solid #fff;
	-webkit-box-shadow: none !important;
}

.ui-inputqty {
	border: 1px solid !important;
	width: 90%;
	height: 40px;
	margin: 0px 0px 0px 15px;
	margin-bottom: 13px;
	border-color: #BBBFBF !important;
	text-align: center;
}

.ui-growl {
	top: 121px;
}

hr {
	border-top: 1px solid #b3aeae;
	width: 95%;
}
.nop{
	padding-right: 0px;
	padding-left: 0px;
}
.weeks {
	font-size: 15px;
    font-weight: bold;
    padding-left: 0px;
    padding-right: 0px;
    height: 40px;
    line-height: 45px;
    margin-left: -17px;
}
.ui-widget {
    font-size: 93%;
}
</style>
<h:body style="background-color: #fff;overflow-x: hidden;">
	<!-- <h:panelGroup id="header">		
<ui:include src="/WEB-INF/include/mobileHeaderTemplate.xhtml"></ui:include>
</h:panelGroup> -->

	<h:form styleClass="form-vertical" id="header">
		<div
			style="background-color: #006DB4; border-bottom: 1px solid #004F8A; height: 60px; padding: 10px 0px; position: fixed; top: 0; z-index: 9999; width: 100%;">
			<div class="container-fluid nop">
				<div class="row">

					<div class="col-xs-1" style="margin-top: 12px;padding-left: 21px;"><h:commandLink value="" action="searchResults.xhtml?faces-redirect=true" style="width:0px;"><h:graphicImage value="resources/app/images/back.svg"></h:graphicImage></h:commandLink></div>

					<!-- <div class="col-xs-7">
						<h:graphicImage value="resources/app/images/logo-hp-white.png"
							style="width:150px;" />
					</div> -->
					<div class="col-xs-7"><h:commandLink value="" action="#{homeBean.mobileHome}" style="width:0px;"><h:graphicImage value="resources/app/images/hplogo.png" style="width:40px;"/><span style="color: #fff;font-size: 17px;font-weight: 600;margin-left: -2px;">CLEVERCHECK</span></h:commandLink></div>
					<div class="col-xs-1" style="margin-top: 9px;padding-left: 0px;"><a href="#{request.contextPath}/search.xhtml"><h:graphicImage value="resources/app/images/search.svg"></h:graphicImage></a></div>
	    			<c:choose>
					<c:when test="#{homeBean.cartItems!=0}">
	    			<!-- <div class="col-xs-1" style="margin-top: -7px;padding-left: 5px;"><a href="#{request.contextPath}/cartDetails.xhtml"><span class="badge" style="margin-left: 10px;padding: 1px 3px;background-color: #d32f2f;border-radius: 50%;">#{homeBean.cartItems}</span><h:graphicImage value="resources/app/images/cart.svg"></h:graphicImage></a> </div> -->
	    			<div class="col-xs-1" style="margin-top: -7px;padding-left: 5px;"><h:commandLink action="#{homeBean.getCartDetails}"><span class="badge" style="margin-left: 10px;padding: 1px 3px;background-color: #d32f2f;border-radius: 50%;">#{homeBean.cartItems}</span><h:graphicImage value="resources/app/images/cart.svg"></h:graphicImage></h:commandLink></div>
	    			</c:when>
	    			<c:otherwise>
	    			<div class="col-xs-1" style="margin-top: -7px;padding-left: 5px;padding-top: 13px;"><a href="#{request.contextPath}/cartDetails.xhtml"><span class="badge" style="margin-left: 10px;padding: 1px 3px;background-color: #d32f2f;border-radius: 50%;"></span><h:graphicImage value="resources/app/images/cart.svg"></h:graphicImage></a> </div>
	    			</c:otherwise>
	    			</c:choose>
	    			<div class="col-xs-1" style="margin-top: 8px;"><h:commandLink value="" action="#{userInfoBean.mobileControlPage}"><h:graphicImage value="resources/app/images/user.svg"></h:graphicImage></h:commandLink> </div>
				</div>
			</div>
		</div>
	</h:form>
	<h:form style="width:100%;margin-top:68px;">
		<!-- <p:commandButton value=""
			styleClass="back_btn glyphicon glyphicon-arrow-left"
			action="searchResults.xhtml?faces-redirect=true" /> -->
		<p:growl id="growl" showDetail="true" life="10000" />
		
		<div align="center" style="margin-top: 20px;">
			<p:graphicImage value="resources/app/images/lap-bg.png"
				rendered="#{addDetailsBean.selectedSkuDetails.imageType == 'NB' }"
				style="height: 60px;height: 90px;" />

			<p:graphicImage value="resources/app/images/sys.png"
				rendered="#{addDetailsBean.selectedSkuDetails.imageType == 'PC' }"
				style="height: 60px;height: 90px;" />

			<p:graphicImage value="resources/app/images/lcd.png"
				rendered="#{addDetailsBean.selectedSkuDetails.imageType == 'WS' }"
				style="height: 60px;height: 90px;" />
		</div>
		
		<div style="margin: 0 10px; font-size: 18px;">
			<p:outputLabel styleClass="text-blue" style="margin-bottom: 0px;"
				value="#{addDetailsBean.selectedSkuDetails.skuId} " />
			<br />
			<p:outputLabel styleClass="text-blue"
				value="#{addDetailsBean.selectedSkuDetails.description}" style="text-overflow: ellipsis;overflow: hidden;"/>
		</div>
		
		<hr/>
		
		<div>
			<div class="row" style="margin-right: 0px;margin-left: 0px;">
				<div class="col-xs-4 nop" style="text-align: center;">
					<label
						style="color: #737373; font-weight: bold; font-size: 11px; margin-bottom: 0px;text-align: center;">Clear
						to Build</label>
					<h5 align="center"
						style="margin-top: 0px; margin-bottom: 4px; font-weight: bold; font-size: 38px;">#{addDetailsBean.selectedSkuDetails.skuAvailability}</h5>
				</div>
				<div class="col-xs-4 nop" style="text-align: center;">
					<label
						style="color: #737373; font-weight: bold; font-size: 11px; margin-bottom: 0px;">#{addDetailsBean.selectedSkuDetails.pipeLineWeeks}
						W Pipeline</label>
					<h5 align="center"
						style="margin-top: 0px; margin-bottom: 4px; font-weight: bold; font-size: 38px;">#{addDetailsBean.selectedSkuDetails.pipeLineQuantity}</h5>
				</div>
				<div class="col-xs-4 nop" style="text-align: center;">
					<label
						style="color: #737373; font-weight: bold; font-size: 11px; margin-bottom: 0px;margin-left: -10px;">New
						Delivery</label>
					<div class="row">
						<div class="col-xs-7" style="margin-top: -5px; margin-bottom: 4px; font-weight: bold; font-size: 36px;;
   							 padding-right: 0px;">#{addDetailsBean.selectedSkuDetails.newOrderDeliveryWeeks} </div>
						<div class="col-xs-5 weeks">Weeks</div>
					</div>
				</div>
			</div>
		</div>
		<hr />
		<div class="config" style="margin: 0 0 0 20px;">
			<h4 class="text-blue">
				<p:outputLabel value="CONFIGURATION" />
			</h4>
			<ul class="list-unstyled"
				style="line-height: 22px; font-size: 11px; font-weight: bold; color: #565656;">
				<ui:repeat var="configurations"
					value="#{addDetailsBean.selectedSkuDetails.configuration}">
					<li style="list-style: initial;">#{configurations}</li>
				</ui:repeat>
			</ul>
		</div>
		<hr />
		<p:panelGrid
			rendered="#{addDetailsBean.selectedSkuDetails.skuAvailability == '0'}">
			<div class="config"
				style="margin: 0 19px 0px 24px;">
				<h4 class="text-blue">
					<p:outputLabel value="SHORTAGE"
						rendered="#{addDetailsBean.selectedSkuDetails.skuAvailability == '0'}" />
				</h4>
				<ul class="list-unstyled"
					style="line-height: 22px; font-size: 11px; font-weight: bold; color: #565656;">
					<ui:repeat var="shortages"
						value="#{addDetailsBean.selectedSkuDetails.shortage}"
						varStatus="status">
						<li style="list-style: initial;">#{shortages}</li>
					</ui:repeat>
				</ul>
			</div>
			<hr/>
		</p:panelGrid>
		<!-- <hr /> -->
		<div class="config" style="margin: 0 0 0 20px;">
			<h4 class="text-blue">
				<p:outputLabel value="PIPELINE DETAILS" style="margin-left: 6px;" />
			</h4>
			<div class="border-bottom"
				style="margin-top: 3px; padding-bottom: 20px; margin-left: 5px; margin-right: 5px;">
				<label class="pull-left"
					style="padding-left: 24px; font-size: 11px;">Ready to ship
					Date</label> 
				<label class="pull-right"
					style="padding-right: 6px; font-size: 11px;">Ready to ship
					Units</label>
			</div>
			<div class="row" style="padding-left: 5px; padding-right: 5px;">

				<div class="col-xs-6">
					<ul class="list-unstyled"
						style="font-size: 11px; font-weight: bold; line-height: 22px; color: #565656">
						<ui:repeat var="date" value="#{addDetailsBean.dates}">
							<li style="list-style: initial;">#{date}</li>
						</ui:repeat>
					</ul>
				</div>
				<div class="col-xs-6">
					<ul class="list-unstyled text-center"
						style="font-size: 11px; font-weight: bold; line-height: 22px; color: #565656">
						<li>#{addDetailsBean.selectedSkuDetails.pavail15Days} Units</li>
						<li>#{addDetailsBean.selectedSkuDetails.pavail30Days} Units</li>
						<li>#{addDetailsBean.selectedSkuDetails.pavail45Days} Units</li>
						<li>#{addDetailsBean.selectedSkuDetails.pavail60Days} Units</li>
						<li>#{addDetailsBean.selectedSkuDetails.pavail75Days} Units</li>
						<li>#{addDetailsBean.selectedSkuDetails.pavail90Days} Units</li>
					</ul>
					
				</div>
					
				</div>
				
		</div>
		<hr style="margin-bottom: 10px;"/>
				<div class="row">
					<div class="col-xs-6"
						style="font-size:22px; font-weight: bold; color: #565656;text-align: center;padding: 0px 0px 0px 35px;">Total</div>
					<div class="col-xs-6"
						style="font-size: 22px; font-weight: bold; color: #565656;text-align: center; padding-left: 35px;">#{addDetailsBean.selectedSkuDetails.pipeLineQuantity}</div>
					
				</div>
				<hr style="margin-top: 10px;"/>
		<div style="padding-bottom: 50px;">
			<p:inputText id="quantity" placeholder="INPUT QTY"
				styleClass="ui-inputqty" value="#{addDetailsBean.quantity}" style="font-size: 18px;"/>
		</div>
		<p:commandButton value="ADD TO CART" style="font-size: 20px;text-shadow:none;"
			actionListener="#{addDetailsBean.addToCart}"
			update="growl @form header" styleClass="addtocart" />

		<script type="text/javascript"
			src="resources/assets/bootstrap/js/bootstrap.min.js"></script>


	</h:form>
</h:body>
</html>