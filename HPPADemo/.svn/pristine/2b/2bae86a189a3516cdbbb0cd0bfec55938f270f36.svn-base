<script type="text/javascript">
	$(document).ready(function(){
		$("body").fadeIn(1000);
		$("body, .swipeArea, .businessUnitsClick, .searchClick").css("height", $(window).height()+50);	
		$(".businessUnitsClick .businessContent, .searchClick .searchData").css("height", $(window).height()-100);
		$(".fa-angle-down").hide();
		animateUpArrowIcon();
		
		$(".ui-accordion-header").click(function(){
			$(".businessUnitsClick .businessHeader  .heading, .businessUnitsClick .businessContent").empty();
			//console.log($(this).next().find("li")[0].innerHTML);
			console.log($(this)[0].nextSibling);
			
			$("body").css("overflow","hidden");
			
			$(".businessUnitsClick, .businessUnitsClick .businessHeader, .businessUnitsClick .businessContent").fadeIn(200);
			
			$(".businessUnitsClick .businessHeader .heading").html($(this).text());
			
			//console.log($(this).next().find("a"));
			
			$(this).next().find("a").each(function(){
				console.log($(this)[0].outerHTML);
				$(".businessUnitsClick .businessContent").append($(this)[0].outerHTML);
			});
			
			
			$(".businessUnitsClick .businessContent").animate({scrollTop:0}, 'fast');
			
			//$(".businessUnitsClick .businessContent").html($(this)[0].nextSibling);
			//.businessUnitsClick .businessHeader .businessUnitsClick .businessContent
			
			$(this).removeClass("ui-state-active");
			$(this).attr("aria-expanded","false");
			$(this).attr("aria-selected","false");
			$(this).next().attr("aria-hidden", "true");
			$(this).next().hide();
		});
		
		$(".businessUnitsClick .businessHeader").click(function(){
			$(".businessUnitsClick, .businessUnitsClick .businessHeader, .businessUnitsClick .businessContent").fadeOut(200);
			$("body").css("overflow-y","scroll");
		});
		
		$(".searchInput").click(function(){
			$(".searchClick").show();
			$(".searchClick input").focus().val('');
			$(".searchClick").animate({
				top: "0px"
			}, 300, function(){

			});
		});
		
		$(".searchClick .fa").click(function(){
			
			$(".searchClick").animate({
				top: "250px"
			}, 100, function(){
				$(".searchClick").fadeOut(100);
			});
		});
		
		/* $(".swipeArea").click(function(){
			openBusinessUnits();
		}); */
		
	});
	
	
	$(function() { 
		$(".swipeArea").swipe( {
		  swipeUp:function(event, direction, distance, duration) {
			  console.log("You swiped " + direction);
		    openBusinessUnits();
		  }	
		});
		
		$(".ui-accordion-header").swipe({
			swipeDown:function(event, direction, distance, duration) {
				  console.log("You swiped " + direction);
				  closeBusinessUnits();
			  }
		});
	});
	function openBusinessUnits() {
		console.log("Business Units");
		$(".searchShrink").animate({
			width: "70%"
		}, 500, function(){

		});
		$(".mainHeaderData").css("background","none");
		$(".mainHeaderData").css("background-color","none");
		$(".mainHeaderData").css("border-bottom","none");
		$(".swipeArea").hide();
		$(".fa-angle-down").show();
		//$(".businessUnitsData .ui-accordion-header").css("height","auto");
		$(".businessUnitsData .ui-accordion-header").css("overflow","none");
		$("body").css("overflow-y","hidden");
		var height = $(window).height()-74-20;
		height = height/5;
		$(".ui-accordion-header").css("line-height",height-16+"px");
		$(".businessUnitsData .ui-accordion-header").animate({
		    height: height+"px"
		  }, 100, function() {
		    // Animation complete.
		  });
		$(".content-body").hide();
		 $(".businessUnitsData").animate({
		    top: "74px"
		  }, 200, function() {
		    // Animation complete.
		  });
	}

	
	//background-color: #2b72b8;border-bottom: 1px solid #004F8A
	
	function animateUpArrowIcon() {
		$(".upArrowIcon").animate({
		    	top: "-19px"
		  }, 400, function() {
			  $(".upArrowIcon").animate({
			    	top: "-22px"
			  }, 400, function() {
				  $(".upArrowIcon").animate({
				    	top: "-15px"
				  }, 800, function() {
					  animateUpArrowIcon();
				  });
			  });
		  });		
	}
	
	function closeBusinessUnits() {
		//console.log("Business Units");
		$("body").css("overflow-y","hidden");
		$(".searchShrink").animate({
			width: "100%"
		}, 500, function(){
			// Call back
		});
		$(".mainHeaderData").css("background-color","#006bb3");
		$(".mainHeaderData").css("border-bottom","1px solid #004F8A");
		$(".swipeArea").show();
		$(".content-body").fadeIn(1000);
		$(".businessUnitsData .ui-accordion-header").css("overflow","hidden");
		$(".businessUnitsData").animate({
		    bottom: "-30px"
		  }, 100, function() {
			  $(".fa-angle-down").hide();
		  });
		$(".businessUnitsData").css("top","inherit");
		$(".businessUnitsData .ui-accordion-header").animate({
		    height: "0.3px"
		  }, 100, function() {
		    // Animation complete.
		  });
	}
	
	function loadcartDetails(page) {
		//alert(page);
		window.location.href = page;
		setTimeout(function(){
			location.reload();
		}, 300);
	}
	
	
	
	 $(function(){
		var ink, d, x, y;
		$(".ripplelink").click(function(e){
	    if($(this).find(".ink").length === 0){
	        $(this).prepend('<span class='ink'></span>');
	    }
	         
	    ink = $(this).find(".ink");
	    ink.removeClass("animate");
	     
	    if(!ink.height()){
	    	if(!ink.width()) {
		        d = Math.max($(this).outerWidth(), $(this).outerHeight());
		        ink.css({height: d, width: d});
	    	}
	    }
	     
	    x = e.pageX - $(this).offset().left - ink.width()/2;
	    y = e.pageY - $(this).offset().top - ink.height()/2;
	     
	    ink.css({top: y+'px', left: x+'px'}).addClass("animate");
	});
	});
	
