package com.techouts.pojo;

import org.springframework.data.annotation.Id;
import org.springframework.data.annotation.TypeAlias;
import org.springframework.data.mongodb.core.mapping.Document;
import org.springframework.data.mongodb.core.mapping.Field;

@Document(collection="FamilyInfoFinal")
@TypeAlias("FamilyInfoFinal")
public class FamilyInfoFinal {
    
    @Id
    private String id;
    @Field("family")
    private String family;
    
    //@Field("commodity")
    //private String commodity;
    @Field("altCommodity")
    private String altCommodity;
    @Field("avId")
    private String avId;
    @Field("description2")
    private String description;
    @Field("quantity")
    private int quantity;
    @Field("leadTime")
    private int leadTime;
    @Field("cost")
    private double cost;
    @Field("isInclude")
    private String isInclude;
    @Field("local/import")
    private String local_import;
    
    @Field("secondary")
    private String secondary;
    @Field("secondaryDesc")
    private String secondaryDesc;
    
    private int isSelectedFamily;
    @Field("includedPart")
    private String includedPart;
    
    
    public String getLocal_import() {
		return local_import;
	}
    public void setLocal_import(String local_import) {
		this.local_import = local_import;
	}
    public String getId() {
        return id;
    }
    public void setId(String id) {
        this.id = id;
    }
    public String getFamily() {
        return family;
    }
    public void setFamily(String family) {
        this.family = family;
    }
    
    public String getIsInclude() {
        return isInclude;
    }
    public void setIsInclude(String isInclude) {
        this.isInclude = isInclude;
    }
    public String getAltCommodity() {
        return altCommodity;
    }
    public void setAltCommodity(String altCommodity) {
        this.altCommodity = altCommodity;
    }
    public String getAvId() {
        return avId;
    }
    public void setAvId(String avId) {
        this.avId = avId;
    }
    public String getDescription() {
        return description;
    }
    public void setDescription(String description) {
        this.description = description;
    }
    public int getQuantity() {
        return quantity;
    }
    public void setQuantity(int quantity) {
        this.quantity = quantity;
    }
    public int getLeadTime() {
        return leadTime;
    }
    public void setLeadTime(int leadTime) {
        this.leadTime = leadTime;
    }
    public double getCost() {
        return cost;
    }
    public void setCost(double cost) {
        this.cost = cost;
    }
    public int getIsSelectedFamily() {
        return isSelectedFamily;
    }
    public void setIsSelectedFamily(int isSelectedFamily) {
        this.isSelectedFamily = isSelectedFamily;
    }
    public String getSecondary() {
        return secondary;
    }
    public void setSecondary(String secondary) {
        this.secondary = secondary;
    }
    public String getSecondaryDesc() {
        return secondaryDesc;
    }
    public void setSecondaryDesc(String secondaryDesc) {
        this.secondaryDesc = secondaryDesc;
    }
	public String getIncludedPart() {
		return includedPart;
	}
	public void setIncludedPart(String includedPart) {
		this.includedPart = includedPart;
	}
    
    
    
    
    

}
